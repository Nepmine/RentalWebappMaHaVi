// Setup ----------------------
generator client {
  provider = "prisma-client-js"
  // output   = "../generated/prisma"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

// Tables -------------------------
model Properties {
  id   Int    @id @default(autoincrement())
  name String
}

model Owner {
  id   Int    @id @default(autoincrement())
  name String
}
